{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.gstatic.com/ https://*.firebaseio.com/; object-src 'self';",

  "name": "NewTabPage",
  "description": "Custom new tab page",
  "version": "2.0",

  "browser_action": {
    "default_title": "David NT",
    "default_icon": "icon.png", 
    "default_popup": "" 
  },

  "icons":{
    "64" : "icon.png"
  }, 

  "chrome_url_overrides": {
    "newtab" : "newtab.html"
  }
}
