{
  "manifest_version": 2,

  "name": "NewTab",
  "description": "Custom new tab page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png", 
    "default_popup": "" 
  },

  "icons":{
    "64" : "icon.png"
  },

  "content_scripts": [
    {
    "matches": ["http://*/*" , "https://*/*"],
    "js": ["js/contentScript.js"]
  }
  ],  

  "background": {
    "scripts": ["js/background.js"]
  },

  "chrome_url_overrides": {
    "newtab" : "newtab.html"

  }
}
